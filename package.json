    "authorization",
    "react"
  ],
  "author": "Aserto",
  "license": "Apache-2.0",
  "peerDependencies": {
    "bootstrap": "^4.5.3",
    "font-awesome": "^4.7.0",
    "react": "^16.14.0",
    "react-bootstrap": "^1.4.3",
    "react-dom": "^16.14.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.8",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
